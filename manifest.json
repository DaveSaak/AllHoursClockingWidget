{
    "manifest_version": 2,
    "name": "AllHoursClockingWidget",
    "description": "clock-in/out from Chrome",
    "version": "1",
    "author": "Dave",

    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "allhours-logo.png"
    },

    "permissions": [
        "storage",
        "notifications",
        "https://dave-allhoursapi2.azurewebsites.net/*"
    
    ]

}